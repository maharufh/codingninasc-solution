//if input is a positive number

#include <iostream>
using namespace std;

int main() {
    //Write your code here
    int i,fact=1,num;      
    cin>>num;   
    
  for(i=1;i<=num;i++){ 
      
      fact=fact*i;    
  }    
  cout<<fact<<endl;  

    return 0;
}
//using for loop
#include <iostream>

using namespace std;

int main() {
    int n, fact;
    cin >> n;
    if (n < 0)
        cout << "Error";
    else {
        fact = 1;
        for (int i = 2; i <= n; i++)
            fact *= i;
        cout << fact;
    }

    return 0;
}

//using while loop
#include <iostream>

using namespace std;

int main() {
    int n;
    cin >> n;
    if (n < 0)
        cout << "Error";
    else if (n == 0)
        cout << 1;
    else {
        int i = n, fact = 1;
        while (n / i != n) {
            fact = fact * i;
            i--;
        }
        cout << fact;

    }

    return 0;
}
